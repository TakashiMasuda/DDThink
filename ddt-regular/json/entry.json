{
    "entryContentArea":{
        "id":"main",
        "class":"entryform",
        "pageSideMenu":{
            "class":"sidemenu recruitSideMenu"
        },
        "maincontents":{
            "id":"maincontents",
            "pageArticle":{
                "id":"entry"
            }
        }
    },
    "supplement": {
        "supplementP": {
            "html": "<span class='text-red'>*</span>は必須入力項目です。"
        }
    },
    "entryForm": {
        "class":"form",
        "action": "ddt-regular/postmail/postmail.cgi",
        "method": "post",
        "entryFormContent": {
            "nameHead": {
                "name": {
                    "class": "formcontent",
                    "html": "氏名<span class='text-red'>*</span>"
                },
                "nameruby": {
                    "class": "formruby",
                    "text": "Name"
                }
            },
            "nameContent": {
                "nameInput": {
                    "class": "text",
                    "type": "text",
                    "name": "name",
                    "size": "40"
                }
            },
            "kanaHead": {
                "kana": {
                    "class": "formcontent",
                    "html": "フリガナ<span class='text-red'>*</span>"
                },
                "kanaruby": {
                    "class": "formruby",
                    "text": "Furigana"
                }
            },
            "kanaContent": {
                "kanaInput": {
                    "class": "text",
                    "type": "text",
                    "name": "furigana",
                    "size": "40"
                }
            },
            "ageHead": {
                "age": {
                    "class": "formcontent",
                    "html": "年齢<span class='text-red'>*</span>"
                },
                "ageruby": {
                    "class": "formruby",
                    "text": "age"
                }
            },
            "ageContent": {
                "ageInput": {
                    "class": "text",
                    "type": "text",
                    "name": "age",
                    "size": "40"
                }
            },
            "sexHead": {
                "sex": {
                    "class": "formcontent",
                    "html": "性別<span class='text-red'>*</span>"
                },
                "sexruby": {
                    "class": "formruby",
                    "text": "sex"
                }
            },
            "sexContent": {
                "sexRadio1": {
                    "type": "radio",
                    "name": "sex",
                    "value": "男"
                },
                "men": {
                    "text": "男"
                },
                "sexRadio2": {
                    "type": "radio",
                    "name": "sex",
                    "value": "女"
                },
                "women": {
                    "text": "女"
                }
            },
            "telHead": {
                "tel": {
                    "class": "formcontent",
                    "html": "電話番号<span class='text-red'>*</span>"
                },
                "telruby": {
                    "class": "formruby",
                    "text": "Tel"
                }
            },
            "telContent": {
                "telInput": {
                    "class": "text",
                    "type": "text",
                    "name": "tel",
                    "size": "40",
                    "maxlength": "12",
                    "pattern": "^[0-9]+$"
                }
            },
            "emailHead": {
                "email": {
                    "class": "formcontent",
                    "html": "メールアドレス<span class='text-red'>*</span>"
                },
                "emailruby": {
                    "class": "formruby",
                    "text": "E-mail"
                }
            },
            "emailContent": {
                "emailInput": {
                    "class": "text",
                    "type": "email",
                    "name": "mailaddress",
                    "size": "40"
                }
            },
            "postcodeHead": {
                "postcode": {
                    "class": "formcontent",
                    "html": "郵便番号<span class='text-red'>*</span>"
                },
                "postcoderuby": {
                    "class": "formruby",
                    "text": "Post Code"
                }
            },
            "postcodeContent": {
                "postcodeLeft": {
                    "class": "postcode",
                    "type": "text",
                    "name": "postcode1",
                    "size": "40",
                    "maxlength": "3"
                },
                "postcodeSpace": {
                    "html": "&nbsp;―&nbsp;"
                },
                "postcodeRight": {
                    "class": "postcode",
                    "type": "text",
                    "name": "postcode2",
                    "size": "40",
                    "maxlength": "4"
                }
            },
            "addressHead": {
                "address": {
                    "class": "formcontent",
                    "html": "住所<span class='text-red'>*</span>"
                },
                "addressruby": {
                    "class": "formruby",
                    "text": "Address"
                }
            },
            "addressContent": {
                "addressInput": {
                    "class": "text",
                    "type": "text",
                    "name": "address",
                    "size": "40"
                }
            },
            "experienceHead": {
                "experience": {
                    "class": "formcontent",
                    "html": "職務経歴<span class='text-red'>*</span>"
                },
                "experienceruby": {
                    "class": "formruby",
                    "text": "Experience"
                }
            },
            "experienceContent": {
                "textareaInput": {
                    "class": "textarea text",
                    "name": "experience",
                    "cols": "40",
                    "rows": "10"
                }
            }
        },
        "submitbuttonarea": {
            "submitbutton": {
                "type": "submit",
                "text": "送信内容の確認"
            }
        }
    }
}